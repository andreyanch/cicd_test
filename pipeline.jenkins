#!groovy

properties([disableConcurrentBuilds()])

pipeline {
    agent {
    label 'master'
    }

    options {
    timestamps()
    }

    stages {
        stage("clean folder for project") {
            steps {
                sh 'rm -rf /home/ec2-user/cicd_test'
            }
        }
        stage("clone git project to local folder") {
            steps {
                sh 'git clone https://github.com/ultramaxim/cicd_test /home/ec2-user/cicd_test'
                sh 'cd /home/ec2-user/cicd_test'
                sh '~'
            }
        }

    }
}
